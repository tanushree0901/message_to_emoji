message = input("<")
words = message.split(' ')
emoji = {
    ":(" : "â˜¹",
    ":)" : "ğŸ˜",
    ";)" : "ğŸ˜‰",
    ">_<" : "ğŸ˜†",
    "-_-" : "ğŸ˜‘",
    "<3" : "â¤",
    "+_+":"ğŸ˜°",
    "^0^":"ğŸ™„",
    "à²¥_à²¥":"ğŸ˜­"
}
output = ""
for word in words:
    output += emoji.get(word,word)+" "
print(output)

